            android:name="com.hyphenate.easeui.ui.EaseShowVideoActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.hyphenate.easeui.ui.EaseShowBigImageActivity"
            android:hardwareAccelerated="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>